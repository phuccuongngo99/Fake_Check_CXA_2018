{
  "name": "True or Fake?",
  "version": "1.0",
  "description": "Detect fakenews on social media",
  "permissions": ["activeTab", "declarativeContent", "storage","https://infocommsociety.com/"],
  "options_page": "options.html",
  "content_security_policy":"script-src 'self' https://apis.google.com; object-src 'self'",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "32": "images/get_started32.png"
    }
  },
  
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js; object-src 'self'",
  "manifest_version": 2
}
